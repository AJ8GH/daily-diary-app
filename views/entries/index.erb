<form action="/entries/new">
  <input type="submit" value="Add Entry">
</form>

<h1>Your entries:</h1>
<ul>
  <% @entries.each do |entry| %>
    <li>
      <%= entry.title %>
      <form action="/entries/<%= entry.id %>">
        <input type="submit" value="View Entry">
      </form>
    </li>
  <% end %>
</ul>
